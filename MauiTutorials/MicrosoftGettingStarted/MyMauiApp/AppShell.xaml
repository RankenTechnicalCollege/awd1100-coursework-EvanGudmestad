<?xml version="1.0" encoding="UTF-8" ?>
<!--Structure of .NET Maui App-->
<!-- This is the main shell of the application. It defines the navigation structure and loads the main page. -->
    <!--Shell handles navigation for content-->
<Shell
    x:Class="MyMauiApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MyMauiApp"
    Title="MyMauiApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <!--<ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:TestPage}"
        Route="TestPage" />-->

</Shell>
